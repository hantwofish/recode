@book{
施智平著关永李黎明-582,
   Author = {施智平著关永李黎明},
   Title = {几何代数的形式化与初步应用},
   Publisher = {北京：科学出版社},
   Abstract = {近年来，各主要国家都把发展人工智能（AI）上升为国家战略，全球范围内迅速掀起了AI研发热潮。然而，在AI真正进入普及应用之前，尚有一系列理论与技术瓶颈亟待突破，机器证明便是其中一个共性基础难题。机器证明的重要环节之一是对数学理论进行形式化描述，从而建立统一的、可用于自动推理的形式化数学体系。本书以几何代数的形式化为基本任务，旨在构建一个兼具代数推理和几何解算能力的统一形式化数学定理体系，并将之初步应用于实际物理问题的证明。},
   Year = {2019} }




@book{
